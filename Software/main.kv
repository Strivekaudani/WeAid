<Facial>
    name: "Facial Recognition Page"
    MDBoxLayout:
        orientation: 'vertical'
        # md_bg_color: 1, 1, 1, 1

        # MDIconButton:
        #     icon: "chevron-left"
        #     user_font_size: "35sp"
        #     pos_hint: {"center_x": .12,"center_y": .95}
        #     on_release:
        #         app.root.current = "Child Page"
        #         root.manager.transition.direction = "right"
        # MDLabel:
        #     text: "Back"
        #     font_size: "18sp"
        #     font_name: "Poppins-Regular.ttf"
        #     pos_hint: {"center_x": .65, "center_y": .949}

        Image:
            id: web_cam
            size_hint: 1, .8
            pos_hint: {"center_x": .5,"center_y": .9}
        Button:
            text: "Veryify"
            background_color: 0,0,0,0
            font_name: "Poppins-Medium.ttf"
            size_hint: .79, .08
            pos_hint: {"center_x": .5, "center_y": .20}
            canvas.before:
                Color:
                    rgb: 226/255, 0, 48/255, 1
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
                    radius: [2]
            on_press: root.buildbuild()
            on_release: root.verify()
        MDLabel:
            id: verification_label
            text: "Verification Uninitiated"
            font_size: "22sp"
            font_name: "Poppins-Medium.ttf"
            color: 120/255, 120/255, 120/255, 1
            pos_hint: {"center_x": .6, "center_y": .12}


