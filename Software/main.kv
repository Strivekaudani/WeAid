<Index>
    name: "Index Page"

    MDLabel:
        text: "Select User"
        font_size: "22sp"
        font_name: "Poppins-Medium.ttf"
        # color: 120/255, 120/255, 120/255, 1
        pos_hint: {"center_x": .6, "center_y": .9}

    Button:
        text: "User"
        background_color: 0,0,0,0
        font_name: "Poppins-Medium.ttf"
        size_hint: .79, .08
        pos_hint: {"center_x": .5, "center_y": .61}
        canvas.before:
            Color:
                rgb: 226/255, 0, 48/255, 1
            RoundedRectangle:
                size: self.size
                pos: self.pos
                radius: [2]
        on_release:
            app.root.current = "Child Page"
            root.manager.transition.direction = "left"

    Button:
        text: "Guardian"
        background_color: 0,0,0,0
        font_name: "Poppins-Medium.ttf"
        size_hint: .79, .08
        pos_hint: {"center_x": .5, "center_y": .4}
        canvas.before:
            Color:
                rgb: 226/255, 0, 48/255, 1
            RoundedRectangle:
                size: self.size
                pos: self.pos
                radius: [2]
        on_release:
            app.root.current = "Login page"
            root.manager.transition.direction = "left"

